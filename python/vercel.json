{
    "version": 2,
    "builds": [
      {
        "src": "api.py",
        "use": "@vercel/python",
        "config": {
          "maxLambdaSize": "50mb",
          "includeFiles": ["llm.py", ".env", "src/react_agent/**"],
          "excludeFiles": ["**/*.pyc", "**/__pycache__/**"]
        }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "api.py"
      }
    ]
  }