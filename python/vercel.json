{
    "version": 2,
    "builds": [
      {
        "src": "api.py",
        "use": "@vercel/python",
        "config": {
          "maxLambdaSize": "50mb",
          "runtime": "python3.9",
          "includedFiles": ["*.py", "src/**/*.py"]
        }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "api.py"
      }
    ]
  }